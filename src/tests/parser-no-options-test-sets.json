{
  "name":"Sets of tests for the parser when no options are set",
  "object": "parser",
  "testSets": [
    {
      "name": "Simple parsing test cases",
      "method": "parse-no-options",
      "testCases": [
        { "n": "Single hash, no spacing",
          "i": "#Heading one",
          "o": "<h1>Heading one</h1>"},
        { "n": "Double hash, no spacing",
          "i": "##Heading two",
          "o": "<h2>Heading two</h2>"},
        { "n": "Triple hash, no spacing",
          "i": "###Heading three",
          "o": "<h3>Heading three</h3>"}
      ]
    }
  ]
}